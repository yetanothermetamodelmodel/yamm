<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated YammDiagram -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="online.yamm.coreModelDiagram.yammDiagram.presentation.YammDiagramModelWizardID"
            name="%_UI_YammDiagramModelWizard_label"
            class="online.yamm.coreModelDiagram.yammDiagram.presentation.YammDiagramModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/YammDiagramModelFile.gif">
         <description>%_UI_YammDiagramModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated YammDiagram -->
      <editor
            id="online.yamm.coreModelDiagram.yammDiagram.presentation.YammDiagramEditorID"
            name="%_UI_YammDiagramEditor_label"
            icon="icons/full/obj16/YammDiagramModelFile.gif"
            extensions="yammdiagram"
            class="online.yamm.coreModelDiagram.yammDiagram.presentation.YammDiagramEditor"
            contributorClass="online.yamm.coreModelDiagram.yammDiagram.presentation.YammDiagramActionBarContributor">
      </editor>
   </extension>

</plugin>
