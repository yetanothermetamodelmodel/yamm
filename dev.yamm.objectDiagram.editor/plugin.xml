<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated ObjectDiagram -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="dev.yamm.ObjectDiagram.presentation.ObjectDiagramModelWizardID"
            name="%_UI_ObjectDiagramModelWizard_label"
            class="dev.yamm.ObjectDiagram.presentation.ObjectDiagramModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ObjectDiagramModelFile.gif">
         <description>%_UI_ObjectDiagramModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated ObjectDiagram -->
      <editor
            id="dev.yamm.ObjectDiagram.presentation.ObjectDiagramEditorID"
            name="%_UI_ObjectDiagramEditor_label"
            icon="icons/full/obj16/ObjectDiagramModelFile.gif"
            extensions="objectdiagram"
            class="dev.yamm.ObjectDiagram.presentation.ObjectDiagramEditor"
            contributorClass="dev.yamm.ObjectDiagram.presentation.ObjectDiagramActionBarContributor">
      </editor>
   </extension>

</plugin>
